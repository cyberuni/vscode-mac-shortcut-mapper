{
	"name": "mac-shortcut-mapper",
	"displayName": "MacOS Keyboard shortcut mapper",
	"version": "0.3.0",
	"description": "Emulate MacOS keyboard shortcuts on Windows with AutoHotKey",
	"categories": [
		"Keymaps"
	],
	"keywords": [
		"vscode",
		"keymap",
		"macos",
		"keyboard",
		"shortcut",
		"autohotkey"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/unional/vscode-mac-shortcut-mapper.git"
	},
	"license": "MIT",
	"publisher": "unional",
	"main": "./out/extension.js",
	"scripts": {
		"check": "biome check",
		"compile": "tsc -p ./",
		"cs": "changeset",
		"pretest": "pnpm run compile && pnpm run check",
		"test": "vscode-test",
		"version": "changeset version",
		"publish": "vsce publish",
		"vscode:prepublish": "pnpm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"command": "macos-key-mapper.helloWorld",
				"title": "Hello World"
			}
		],
		"configuration": {
			"title": "MacOS Key Mapper",
			"type": "object",
			"properties": {
				"macos-key-mapper.autoHotKeyPath": {
					"type": "string",
					"default": "C:\\Program Files\\AutoHotkey\\AutoHotkey.exe",
					"description": "Path to AutoHotKey executable"
				}
			}
		}
	},
	"activationEvents": [
		"onStartupFinished"
	],
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@changesets/cli": "^2.28.1",
		"@repobuddy/biome": "^1.7.1",
		"@types/mocha": "^10.0.10",
		"@types/node": "20.x",
		"@types/vscode": "^1.80.0",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1",
		"typescript": "^5.7.3"
	},
	"packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
	"engines": {
		"vscode": "^1.80.0"
	},
	"preview": true,
	"extensionKind": [
		"ui"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"@biomejs/biome"
		]
	}
}
