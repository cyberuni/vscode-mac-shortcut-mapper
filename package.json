{
  "name": "mac-shortcut-mapper",
  "displayName": "MacOS Keyboard shortcut mapper",
  "version": "0.0.3",
  "description": "Emulate MacOS keyboard shortcuts on Windows with AutoHotKey",
  "categories": [
    "Keymaps"
  ],
  "keywords": [
    "vscode",
    "keymap",
    "macos",
    "keyboard",
    "shortcut",
    "autohotkey"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unional/vscode-mac-shortcut-mapper.git"
  },
  "license": "MIT",
  "publisher": "unional",
  "main": "./out/extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src",
    "pretest": "pnpm run compile && pnpm run lint",
    "test": "vscode-test",
    "vscode:prepublish": "pnpm run compile",
    "watch": "tsc -watch -p ./"
  },
  "contributes": {
    "commands": [
      {
        "command": "macos-key-mapper.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@repobuddy/biome": "^1.7.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@10.6.3+sha512.bb45e34d50a9a76e858a95837301bfb6bd6d35aea2c5d52094fa497a467c43f5c440103ce2511e9e0a2f89c3d6071baac3358fc68ac6fb75e2ceb3d2736065e6",
  "engines": {
    "vscode": "^1.80.0"
  },
  "preview": true,
  "extensionKind": [
    "ui"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome"
    ]
  }
}
